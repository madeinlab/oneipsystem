config password_rule
    option min_length_default '9'
    option max_length_default '32'
    option min_length '9'
    option max_length '32'

config login_rule
    option retry_count_default '5'
    option retry_interval_default '5'
    option retry_count '5'
    option retry_interval '5'

config admin_ip 'config'
    option enabled '0'
    list allowed_ips ''
