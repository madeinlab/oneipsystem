{
	"luci-app-acl": {
		"description": "Grant access to ACL configuration",
		"read": {
			"cgi-io": [ "list", "read" ],
			"file": {
				"/etc/passwd": [ "read" ],
				"/usr/bin/openssl": [ "list" ],
				"/usr/bin/openssl passwd -6 -stdin *": [ "exec" ],
				"/bin/sh -c *": [ "exec" ],
				"/usr/share/rpcd/acl.d": [ "list" ],
				"/usr/share/rpcd/acl.d/*.json": [ "read" ]
			},
			"uci": [ "rpcd" ]
		},
		"write": {
			"uci": [ "rpcd" ]
		}
	}
}
